app dishes {
  wasp: {
    version: "^0.11.5"
  },
  title: "dishes"
}

route RootRoute { path: "/", to: MainPage }
page MainPage {
  component: import Main from "@client/MainPage.jsx"
}

entity Task {=psl
  id          Int     @id @default(autoincrement())
  description String  
  isDone      Boolean @default(false)
psl=}

query getTasks {
  fn: import { getTasks } from "@server/queries.js",
  entities: [Task]
}
